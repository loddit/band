do(global = this) ->
  DRUM_SOUNDS =
    kick: new Audio("data:audio/wav;base64,")
    snare: new Audio("data:audio/wav;base64,")
    hatOpen: new Audio("data:audio/wav;base64,")
    hatClose: new Audio("data:audio/wav;base64,")
    clap: new Audio("data:audio/wav;base64,")
    bongo1: new Audio("data:audio/wav;base64,")
    bongo2: new Audio("data:audio/wav;base64,")

  playDrum = (number) ->
    soundArray = [
      DRUM_SOUNDS.kick
      DRUM_SOUNDS.snare
      DRUM_SOUNDS.hatOpen
      DRUM_SOUNDS.hatClose
      DRUM_SOUNDS.bongo1
      DRUM_SOUNDS.bongo2
      DRUM_SOUNDS.clap
    ]
    soundArray[number].play()

  global.playDrum = playDrum
